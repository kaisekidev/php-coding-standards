{
    "name": "kaiseki/php-coding-standard",
    "description": "Kaiseki PHP Coding Standard",
    "license": "MIT",
    "type": "phpcodesniffer-standard",
    "authors": [
        {
            "name": "Kaiseki",
            "email": "hello@kaiseki.dev",
            "homepage": "https://www.kaiseki.dev",
            "role": "Organization"
        },
        {
            "name": "David Mondok",
            "email": "david@woda.at",
            "homepage": "https://github.com/davidmondok",
            "role": "Developer"
        },
        {
            "name": "Wolfgang Schaefer",
            "email": "wolfgang@woda.at",
            "homepage": "https://github.com/wolfgangschaefer",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5 || ^0.6 || ^0.7",
        "slevomat/coding-standard": "^8.4",
        "squizlabs/php_codesniffer": "^3.7.1",
        "webimpress/coding-standard": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Kaiseki\\CodingStandard\\": "Kaiseki"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.21"
    }
}
